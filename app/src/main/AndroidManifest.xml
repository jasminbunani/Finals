<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jassyjaaas.bunani_final">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="com.example.jassyjaaas.bunani_final.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SignUp">
            <intent-filter>
                <action android:name="com.example.jassyjaaas.bunani_final.SignUp" />
            </intent-filter>
        </activity>

        <activity
            android:name=".OnTouch">
            <intent-filter>
                <action android:name="com.example.jassyjaaas.bunani_final.OnTouch" />
            </intent-filter>
        </activity>

    </application>

</manifest>